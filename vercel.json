{
    "version": 2,
    "builds": [
        {
            "src": "bin/www.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bin/www.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/*": {
            "handler": "bin/www.js"
        }
    }
}